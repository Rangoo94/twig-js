{
    "globals": {
        // Jasmine
        "jasmine": false,
        "describe": false,
        "it": false,
        "beforeEach": false,
        "afterEach": false,
        "xit": false,
        "xdescribe": false,

        // Modules
        "module": true,
        "exports": true,
        "require": false
    },

    "node": true,
    "esnext": true,

    "camelcase": true,
    "eqeqeq": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "curly": true,
    "forin": true,
    "immed": true,
    "maxcomplexity": 7,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "notypeof": true,
    "shadow": false,

    "eqnull": true
}